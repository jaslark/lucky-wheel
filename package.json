    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/jquery": "^3.3.34",
    "@types/luxon": "^1.22.0",
    "@types/node": "^12.12.34",
    "angular-gettext-cli": "^1.2.0",
    "codelyzer": "^6.0.0",
    "husky": "^3.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
