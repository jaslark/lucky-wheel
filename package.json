{
  "name": "aqua-luckywheel",
  "version": "1.0.0",
  "description": "Luckywheel",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=local --o --host 0.0.0.0",
    "start:dev": "ng serve --configuration=development --o --host 0.0.0.0",
    "start:prod": "ng serve --configuration=production --o --host 0.0.0.0",
    "build:local": "ng build --configuration=local",
    "build:dev": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --prod --configuration=development  --base-href \"/\" --output-hashing=all && rimraf dist && mv dist-temp dist && npm run post-build",
    "pwa:build:local": "ng build --configuration=local && sw-precache --root=dist --config=precache-config.js",
    "pwa:build:dev": "ng build --prod --configuration=development && sw-precache --root=dist --config=precache-config.js",
    "pwa:build:prod": "ng build --prod && sw-precache --root=dist --config=precache-config.js",
    "test": "ng test",
    "e2e": "ng e2e",
    "generate": "ng generate",
    "compodoc:gen": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "compodoc:serve": "./node_modules/.bin/compodoc -s src/tsconfig.app.json",
    "compodoc": "npm run compodoc:gen && npm run compodoc:serve",
    "precommit": "lint-staged",
    "lint": "tslint --fix --config tslint.json --project tsconfig.json",
    "format:fix": "pretty-quick --staged",
    "post-build": "node ./build/post-build.js",
    "buildMemory": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --prod --configuration=development --base-href \"/admin/\" --output-hashing=all && rimraf dist && mv dist-temp dist && npm run post-build",
    "build:demo": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --prod --configuration=demo --base-href \"/admin/\" --output-hashing=all && rimraf dist && mv dist-temp dist && npm run post-build",
    "build:prod": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --prod --configuration=production --base-href \"/\" --output-hashing=all && rimraf dist && mv dist-temp dist && npm run post-build",
    "build:uat": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --prod --configuration=uat --base-href \"/\" --output-hashing=all && rimraf dist && mv dist-temp dist && npm run post-build"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "pretty-quick --staged",
      "tslint --fix --config tslint.json --project tsconfig.json",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com"
  },
  "keywords": [
    "angular",
    "angular8",
    "angularjs",
    "angular.io"
  ],
  "author": "Khoa Nguyen <khoanguyenit@gmail.com>",
  "license": "ISC",
